<POLICIES xmlns="http://www.w3.org/2002/01/P3Pv1">
	<POLICY discuri="/w3c/FilthyPolicy.html" name="FilthyPolicy">
		<ENTITY>
			<DATA-GROUP>
				<DATA ref="#business.name">Author</DATA>
				<DATA ref="#business.contact-info.online.email">author@domain.com</DATA>
				<DATA ref="#business.contact-info.online.uri">domain.com</DATA>
				<DATA ref="#business.contact-info.telecom.telephone.number"></DATA>
				<DATA ref="#business.contact-info.postal.street"></DATA>
				<DATA ref="#business.contact-info.postal.city"></DATA>
				<DATA ref="#business.contact-info.postal.stateprov"></DATA>
				<DATA ref="#business.contact-info.postal.postalcode"></DATA>
			</DATA-GROUP>
		</ENTITY>
		<ACCESS>
			<all/>
		</ACCESS>
		<DISPUTES-GROUP>
			<DISPUTES resolution-type="service" service="http://" short-description="Disputes">
				<LONG-DESCRIPTION></LONG-DESCRIPTION>
				<REMEDIES>
					<correct/>
				</REMEDIES>
			</DISPUTES>
		</DISPUTES-GROUP>
		<STATEMENT>
			<CONSEQUENCE>Session management.</CONSEQUENCE>
			<PURPOSE>
				<current required="always"/>
				<admin required="always"/>
			</PURPOSE>
			<RECIPIENT>
				<ours>
					<recipient-description>ours</recipient-description>
				</ours>
			</RECIPIENT>
			<RETENTION>
				<indefinitely/>
			</RETENTION>
			<DATA-GROUP>
				<DATA ref="#dynamic.clickstream"/>
				<DATA ref="#dynamic.clickstream.clientip"/>
				<DATA ref="#dynamic.clickstream.clientip.fullip"/>
				<DATA ref="#dynamic.clickstream.other"/>
			</DATA-GROUP>
		</STATEMENT>
	</POLICY>
</POLICIES>